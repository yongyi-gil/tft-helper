  "synergy": {
    "이모코어": {
      "desc": "이모코어 챔피언의 스킬 마나 소모량이 감소하고 아군 챔피언이 죽을 때마다 마나를 얻습니다.",
      "abilities": {
        "2": "20% 감소, 마나 10 획득",
        "4": "25% 감소, 마나 20 획득",
        "6": "주문력 20, 30% 감소, 마나 25 획득"
      }
    },
    "True Damage": {
      "desc": "True Damage 챔피언이 추가 고정 피해를 입힙니다. True Damage 챔피언이 아이템을 보유 중일 경우 스킬에 고유 반짝반짝 보너스가 추가됩니다.",
      "abilities": {
        "2": "피해량 15%",
        "4": "피해량 30%",
        "6": "피해량 45%",
        "9": "피해량 125%. 반짝반짝 보너스가 플래티넘이 됩니다!"
      }
    },
    "컨트리": {
      "desc": "아군이 30%의 체력을 잃으면 공포마를 불러 아군을 강화합니다. 컨트리 별 레벨 하나당 공포마의 체력과 공격력이 증가합니다.",
      "abilities": {
        "3": "공포마",
        "5": "한밤의 공포마",
        "7": "지옥의 기원 공포마"
      }
    },
    "8비트": {
      "desc": "아군이 입힌 피해를 기록합니다. 최고 기록을 경신할 때마다 8비트 챔피언의 공격력이 증가합니다.",
      "abilities": {
        "2": "4.5% 공격속도",
        "4": "7.5% 공격속도",
        "6": "12% 공격속도 및 큰 보상을 받는 최종 최고 기록을 얻습니다."
      }
    },
    "하이퍼팝": {
      "desc": "하이퍼팝 챔피언이 스킬 사용 시 가장 가까운 아군 2명에게 마나를 부여하고 4초 동안 공격 속도를 증가시킵니다.",
      "abilities": {
        "1": "3 마나 및 10% 공격속도",
        "2": "5 마나 및 20% 공격속도",
        "3": "7 마나 및 35% 공격속도",
        "4": "10 마나 및 60% 공격속도"
      }
    },
    "펜타킬": {
      "desc": "펜타킬 챔피언이 받는 피해가 15% 감소하고 추가 피해를 입힙니다. 챔피언을 처치할 때마다 펜타킬 챔피언이 록의 혼을 불태워 추가 피해량이 25% 증가합니다.\r\n5번째 처치마다 모든 펜타킬 챔피언이 록의 혼을 불태워 아군이 50%의 공격 속도를 얻습니다.",
      "abilities": {
        "3": "추가 피해량 15%",
        "5": "추가 피해량 30%",
        "7": "추가 피해량 45%",
        "10": "피해량 감소 50%, 추가 피해량 110%"
      }
    },
    "일류비트": {
      "desc": "(고유) 일라오이의 별 레벨에 따라 전장에 놓을 수 있는 2/2/8성 영혼 촉수를 획득합니다. 촉수는 일라오이의 추가 방어력 및 마법 저항력을 얻습니다.",
      "abilities": {}
    },
    "와일드카드": {
      "desc": "(고유) 플레이어 대상 전투에서 승리 시 케인이 그림자 암살자가 됩니다. 그러지 못할 경우에는 라아스트가 됩니다. 케인이 적 챔피언 2명을 처치할 때마다 형태에 따라 보상을 획득합니다.\r\n그림자 암살자: 3골드\r\n라아스트: 플레이어 체력 1",
      "abilities": {}
    },
    "믹스마스터": {
      "desc": "(고유) 모드를 선택하여 믹스마스터의 기본 공격과 스킬을 변화시킵니다.",
      "abilities": {}
    },
    "KDA": {
      "desc": "빛나는 칸에 있는 아군이 최대 체력과 주문력, 공격력을 얻습니다. K/DA 챔피언일 경우 증가량이 두 배가 됩니다.",
      "abilities": {
        "3": "최대 체력의 10%, 주문력, 공격력 10%",
        "5": "최대 체력의 15%, 주문력, 공격력 15%",
        "7": "최대 체력의 22%, 주문력, 공격력 220%",
        "10": "최대 체력의 50%, 주문력, 공격력 50%, 초당 마나 10"
      }
    },
    "마에스트로": {
      "desc": "(고유) 마에스트로는 항상 고정된 속도로 공격하며 1%의 추가 공격 속도를 0.699999988079071%의 공격력으로 전환합니다. ",
      "abilities": {}
    },
    "디스코": {
      "desc": "이동 가능한 디스코 볼을 소환합니다.\r\n전투 시작: 디스코 볼 옆에 있는 아군이 공격 속도를 얻습니다. 또한 전투 시작 즉시, 이후 3초마다 체력을 회복합니다.",
      "abilities": {
        "3": "공격속도 5%, 최대체력의 2%",
        "4": "공격속도 10%, 최대체력의 3%",
        "5": "디스코 볼 2개, 공격속도 15%, 최대체력의 3%",
        "6": "공격속도 20%, 최대체력의 5%"
      }
    },
    "펑크": {
      "desc": "펑크가 추가 체력과 공격력을 얻습니다. 상점 새로고침에 골드를 소모할 때마다 효과가 1% 증가합니다.\r\n펑크가 전투에 참여한 후 첫 상점 새로고침 비용이 1골드가 되고 3%의 보너스를 얻습니다.",
      "abilities": {
        "2": "체력 170 및 공격력 17%",
        "4": "체력 280 및 공격력 28%",
        "6": "체력 420 및 공격력 42%"
      }
    },
    "Heartsteel": {
      "desc": "적을 처치하면 하트를 획득합니다. 플레이어 대상 전투에서 패배하면 더 많이 얻습니다. 매 4번째 플레이어 대상 전투에서 하트가 강력한 보상으로 전환됩니다.",
      "abilities": {
        "3": "하트 획득 1배",
        "5": "하트 획득 2.5배",
        "7": "하트 획득 6배",
        "10": "하트 획득량이 10배가 됩니다. 라운드마다 보상을 획득하고 하트의 80%를 유지합니다."
      }
    },
    "재즈": {
      "desc": "활성화된 특성 하나당 아군이 추가 체력과 추가 피해량을 얻습니다. (고유 특성 제외)",
      "abilities": {
        "2": "체력 1.5%, 피해량 1%",
        "3": "체력 2%, 피해량 1.5%",
        "4": "체력 3%, 피해량 2%"
      }
    },
    "EDM": {
      "desc": "EDM 선택기를 사용해 EDM 챔피언을 선택하고 각 챔피언의 스킬 사용 빈도를 확인할 수 있습니다.\r\n선택한 챔피언의 스킬 사용 빈도에 따라 아군 EDM 챔피언이 해당 스킬을 변경된 효과가 적용된 상태로 사용합니다.",
      "abilities": {
        "2": "80%의 효과",
        "3": "100%의 효과",
        "4": "110%의 효과, 스킬 사용 빈도 -1초",
        "5": "125%의 효과, 스킬 사용 빈도 -2초"
      }
    },
    "감시자": {
      "desc": "아군이 방어력과 마법 저항력을 얻습니다. 감시자는 효과가 두 배가 됩니다.",
      "abilities": {
        "2": "방어력 16, 마법 저항력 16",
